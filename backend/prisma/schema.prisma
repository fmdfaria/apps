  updatedAt    DateTime? @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)

  @@unique([entidadeTipo, entidadeId, contexto, chave])
  @@index([entidadeTipo, entidadeId], map: "idx_configuracoes_entidade")
