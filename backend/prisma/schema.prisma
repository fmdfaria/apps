model Configuracao {
  id           String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  entidadeTipo String    @map("entidade_tipo") @db.VarChar(50)
  entidadeId   String?   @map("entidade_id") @db.Uuid
  contexto     String    @db.VarChar(100)
